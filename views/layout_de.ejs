                <script type="text/javascript" src="/javascripts/google.map.config.js"></script>
                <!-- Custom Scripts -->
                <script type="text/javascript" src="/javascripts/custom.js"></script>
                <script type="text/javascript" src="/javascripts/set_language.js"></script>
